---
import { t } from "i18next"
// @ts-ignore
import { YouTube } from '@astro-community/astro-embed-youtube';

const currentYear = new Date().getFullYear();
---

<section class="container mt-5">
  <h2 class="text-center mb-3">{t("common.nye")} {currentYear}</h2>
  <YouTube id="67R0f49RI60" class="youtube-video" />
</section>

<style>
  .youtube-video {
    width: 100%;
    height: 100%;
    max-width: 100%;
    max-height: 100%;
  }
</style>
