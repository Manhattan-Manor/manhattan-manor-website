---
import { changeLanguage } from "i18next";
import OurTeam from "../components/OurTeam.astro";
import Services from "../components/Services.astro";
import UpcomingEvents from "../components/UpcomingEvents.astro";
import Venues from "../components/Venues.astro";
import VideoBanner from "../components/VideoBanner.astro";
import WhoWeAre from "../components/WhoWeAre.astro";
import Layout from "../layouts/Layout.astro";
import Footer from "../components/Footer.astro";
import Gallery from "../components/Gallery.astro";
import FindUs from "../components/FindUs.astro";
import BallDrop from "../components/BallDrop.astro";
import Countdown from "../components/Countdown.astro";
import NewYearsEve from "../components/NewYearsEve.astro";
import News from "../components/News";
import ContactUs from "../components/ContactUs.astro";
import InstagramFeed from "../components/InstagramFeed.astro";
import Testimonials from "../components/Testimonials";
import Clients from "../components/Clients";
import PreferredVendors from "../components/PreferredVendors";
import MediaPartners from "../components/MediaPartners";
import InTheNews from "../components/InTheNews";
import PartnersHighlights from "../components/PartnersHighlights.astro";

changeLanguage("en");
---

<Layout>
  <VideoBanner />
  <WhoWeAre />
  <Venues />
  <Services />
  <OurTeam />
  <News client:only="react" />
  <PartnersHighlights />
  <UpcomingEvents />
  <InstagramFeed />
  <Gallery />
  <Testimonials client:visible />
  <FindUs />
  <Clients client:idle />
  <PreferredVendors client:idle />
  <MediaPartners client:idle />
  <InTheNews client:idle />
  <BallDrop />
  <Countdown />
  <NewYearsEve />
  <ContactUs />
  <Footer />
</Layout>
